@using System.Web.Mvc.Html

@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="cover">
    <h1 class="text-center">Warehouse Management System</h1>
    <h2 class="text-center">Basic application with ASP.NET MVC 4</h2>
    <hr>
    <ul class="list-inline list-unstyled text-center">
        <li>
            <a class="btn btn-default btn-lg" href="@Url.Action("Login", "Account")"
                role="button">Sign in</a>
        </li>
        <li>
            <a class="btn btn-primary btn-lg" href="@Url.Action("Register", "Account")"
                role="button">Sign up</a>
        </li>
    </ul>
</div>

<script type="text/javascript" src="~/Scripts/knockout-3.4.2.js"></script>
<script type="text/javascript" src="~/Scripts/jquery-2.0.3.js"></script>
<script type="text/javascript" src="~/Scripts/ViewModels/CoverViewModel.js"></script>
